  import pandas as pd

  idx = pd.date_range(start='16/7/2017', end='20/6/2019' ,freq='D')

  pt_2017        = pd.date_range(start='2017-07-16', end='2017-07-23', freq='D')
  ru_2017        = pd.date_range(start='2017-08-04', end='2017-08-21', freq='D')
  ee_2017        = pd.date_range(start='2017-10-27', end='2017-11-01', freq='D')
  hu_2017_2018   = pd.date_range(start='2017-12-27', end='2018-01-03', freq='D')
  pt_march_2018  = pd.date_range(start='2018-03-06', end='2018-03-27', freq='D')
  ee_2018        = pd.date_range(start='2018-05-10', end='2018-05-14', freq='D')
  pt_summer_2018 = pd.date_range(start='2018-06-20', end='2018-07-21', freq='D')
  pt_autumn_2018 = pd.date_range(start='2018-09-18', end='2018-10-25', freq='D')
  ru_2018_2019   = pd.date_range(start='2018-12-28', end='2019-01-08', freq='D')
  pt_2019        = pd.date_range(start='2019-03-03', end='2019-04-26', freq='D')
  ru_2019        = pd.date_range(start='2019-06-25', end='2019-07-21', freq='D')

  ts = pd.Series(data=False, index=idx)

  return idx.intersection(pt_2017)
